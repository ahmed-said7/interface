<html>
    <head>
        <title>My Webpage</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <form action="http://localhost:4000/insert/post" method="POST">
            <label for="lat">Latitude:</label>
            <input type="number" id="lat" name="lat"><br><br>
            <label for="lng">Longitude:</label>
            <input type="number" id="lng" name="lng"><br><br>
            <label for="user_id">User ID:</label>
            <input type="number" id="user_id" name="user_id"><br><br>
            <label for="caption">Caption:</label>
            <textarea id="caption" name="caption"></textarea><br><br>
            <label for="url">URL:</label>
            <input type="text" id="url" name="url"><br><br>
            <button type="submit">Create Post</button>
        </form>
        <a href="http://localhost:4000/home">Back to Home</a>
    </body>
</html>